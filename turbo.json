{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "tsconfig.json"],
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": ["lint"]
    },
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["lint:language"]
    },
    "lint:language": {},
    "//#lint:language": {}
  }
}
